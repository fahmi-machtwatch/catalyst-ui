{
  "name": "@catalyst-ui/account-platform",
  "version": "1.0.0-canary-1",
  "description": "Catalyst Component UI",
  "main": "dist/index.js",
  "module": "dist/account-platform.esm.js",
  "typings": "dist/index.d.ts",
  "catalyst:src": "./src/index.ts",
  "repository": "https://github.com/machtwatch/catalyst-ui.git",
  "author": "Catalyst Tech",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdx build --transpileOnly --tsconfig tsconfig.build.json",
    "build-storybook": "build-storybook --quiet",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublish": "yarn build",
    "report-coverage": "codecov -f ./coverage/*.json",
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/system": "^2.1.3",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "framer-motion": "^6.3.13",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.18.6",
    "@chakra-ui/storybook-addon": "^4.0.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/theming": "^6.5.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "codecov": "^3.8.3",
    "jest": "^26.6.3"
  }
}
